{
    "env": {
        "mocha": true,
        "node": true,
        "es6": true
    },
    "settings": {
        "ecmascript": 6
    },
    "rules": {
        "strict": 0,
        "quotes": 0,
        "no-unused-vars": 2,
        "no-var": 2,
        "camelcase": 2,
        "no-underscore-dangle": 2,
        "no-shadow": 2,
        "consistent-return": 2,
        "no-extra-parens": 1,
        "no-use-before-define": 0
    }
}
